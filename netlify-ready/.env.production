# Supabase Configuration
VITE_SUPABASE_URL=${SUPABASE_URL}
VITE_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}

# API Keys
VITE_DEFAULT_GEMINI_API_KEY=${GEMINI_API_KEY}

# Application Configuration
VITE_APP_NAME=EchoMind
VITE_APP_DESCRIPTION=A mental well-being platform for individuals managing depression
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true

# Security
VITE_ALLOWED_ORIGINS=https://echomind.netlify.app
VITE_CSP_MODE=strict

# Analytics
VITE_GA_MEASUREMENT_ID=${GA_MEASUREMENT_ID}

# Push Notifications
VITE_VAPID_PUBLIC_KEY=${VAPID_PUBLIC_KEY}
