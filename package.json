{
  "name": "echomind-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ts": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server.js",
    "start:dev": "vite",
    "serve": "node server.js",
    "test": "vitest run",
    "test:minimal": "vitest run --config vitest-minimal.config.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run src/__tests__/e2e/*.test.{js,ts,jsx,tsx} --config vitest-minimal.config.ts",
    "verify:env": "node verify-env.js",
    "verify:rls": "node verify-rls.js",
    "security:audit": "node security-audit.js",
    "analyze": "vite-bundle-analyzer",
    "deploy": "npm run build && npm run serve",
    "netlify:fix": "node netlify-fix.cjs"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@supabase/supabase-js": "^2.49.4",
    "chart.js": "^4.4.9",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "formik": "^2.4.6",
    "helmet": "^7.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "uuid": "^9.0.1",
    "vite-plugin-pwa": "^1.0.0",
    "workbox-window": "^7.3.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "ts-jest": "^29.3.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.9.0",
    "vitest": "^3.1.3"
  }
}
